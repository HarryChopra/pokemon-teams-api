Table trainer as T {
  id serial [pk] // bigserial 
  name varchar [NOT NULL]
  created_at timestampz [NOT NULL, default:`now()`]
}

Table pokemon as P {
  id serial [pk]
  trainer_id serial [NOT NULL]
  species varchar [NOT NULL]
  nickname varchar [NOT NULL]
  created_at timestampz [NOT NULL, default:`now()`]
}

